.js",["79"],"import './CartScreen.css'\r\nimport CartItem from '../Components/CartItem'\r\nimport {useEffect} from 'react'\r\nimport { useDispatch, useSelector } from 'react-redux'\r\nimport {Link} from 'react-router-dom'\r\nimport { addToCart, removeFromCart } from '../actions/cartActions'\r\nconst CartScreen=()=>{\r\n    const dispatch =useDispatch();\r\n    const cart =useSelector((state)=>state.cart)\r\n    const {cartItems}=cart;\r\n\r\n    const qtyChangeHandler=(id,qty)=>{\r\n        dispatch(addToCart(id,qty))\r\n    }\r\n    const removeHandler=(id)=>{\r\n        dispatch(removeFromCart(id))\r\n    }\r\n    const getCartCount=()=>{\r\n        return cartItems.reduce((qty,item)=>Number(item.qty)+qty,0)\r\n    }\r\n    const getCartSubTotal=()=>{\r\n        return cartItems.reduce((price,item)=>(Number(item.price)*Number(item.qty))+price,0)\r\n    }\r\n    return(\r\n        <div className=\"cartscreen\">\r\n            <div className=\"cartscreen__left\">\r\n                <h2>Shopping Cart</h2>\r\n                {cartItems.length===0 ? (\r\n                    <div>\r\n                        Your cart is empty <Link to=\"/\">Go Back</Link>\r\n                    </div>\r\n\r\n                ) : (cartItems.map((item)=><CartItem key={item.product} \r\n                item={item} \r\n                qtyChangeHandler={qtyChangeHandler}\r\n                removeHandler={removeHandler}\r\n                ></CartItem>))}\r\n\r\n            </div>\r\n            <div className=\"cartscreen__right\">\r\n                <div className=\"cartscreen__info\">\r\n                    {/* <p> ({cartItems.length}) Products</p> */}\r\n                    <p> ({getCartCount()}) Units from ({cartItems.length}) Product Category</p>\r\n                    <p>Rs.{getCartSubTotal().toFixed(2)*80}</p>\r\n\r\n\r\n                </div>\r\n                <div>\r\n                <button>Proceed to checkout</button>\r\n                </div>\r\n                \r\n                \r\n\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default CartScreen","D:\\sunil\\FullStackProject\\Jigsaw FSIT MERN\\mineit\\frontend\\src\\screens\\ProductScreen.js",["80"],"import './ProductScreen.css'\r\nimport {useState,useEffect} from 'react'\r\nimport {useDispatch,useSelector} from 'react-redux'\r\n\r\nimport {getProductDetials} from '../actions/productActions'\r\nimport {addToCart} from '../actions/cartActions'\r\nimport Product from '../Components/Product'\r\n\r\n\r\nconst ProductScreen=({match,history})=>{\r\n    const [qty,setQty]=useState(1);\r\n    const dispatch=useDispatch()\r\n    \r\n    const productDetails=useSelector((state)=>state.getProductDetails)\r\n    const {loading,error,product}=productDetails;\r\n    useEffect(()=>{\r\n        if(product && match.params.id!==product._id){\r\n            dispatch(getProductDetials(match.params.id))\r\n        }\r\n    },[dispatch,product,match])\r\n\r\n    const addToCartHandler=() =>{\r\n        dispatch(addToCart(product._id,qty))\r\n        history.push('/cart')\r\n    }\r\n    return(\r\n        <div className=\"productscreen\">\r\n            {loading ? (<h2>Loading...</h2>) : error ? (<h2>{error}</h2>) : (\r\n                <>\r\n                <div className=\"productscreen__left\">\r\n                <div className=\"left__image\">\r\n                    <img src={product.imageUrl} alt={product.name}></img>\r\n\r\n                </div>\r\n                <div className=\"left__info\">\r\n                    <p className=\"left__name\">{product.name}</p>\r\n                    <p>Price: {Number(product.price)*80}/-</p>\r\n                    <p>{product.description}</p>\r\n\r\n                </div>\r\n\r\n\r\n            </div>\r\n            <div className=\"productscreen__right\">\r\n                <div className=\"right__info\">\r\n                    <p>\r\n                        Price : <span>{Number(product.price)*80}</span>\r\n                    </p>\r\n                    <p>\r\n                        Status: <span>{product.countInStock > 0 ? \"In Stock\"\r\n                    : \"Out of stock\"    \r\n                    }</span>\r\n                    </p>\r\n                    <p>\r\n                        Quantity\r\n                        <select value={qty} onChange={(e)=>setQty(e.target.value)}>\r\n                            {[...Array(product.countInStock).keys()].map((x)=>(\r\n                                <option key= {x+1} value={x+1}>{x+1}</option>\r\n                            ))}\r\n                            \r\n\r\n                        </select>\r\n                        \r\n                    </p>\r\n                    <p>\r\n                        <button type=\"button\" onClick={addToCartHandler}>Add To Cart</button>\r\n                    </p>\r\n\r\n                </div>\r\n            </div>\r\n            </>\r\n            )}\r\n            \r\n        </div>\r\n    )\r\n}\r\n\r\nexport default ProductScreen","D:\\sunil\\FullStackProject\\Jigsaw FSIT MERN\\mineit\\frontend\\src\\Components\\Navbar.js",[],"D:\\sunil\\FullStackProject\\Jigsaw FSIT MERN\\mineit\\frontend\\src\\Components\\BackDrop.js",[],"D:\\sunil\\FullStackProject\\Jigsaw FSIT MERN\\mineit\\frontend\\src\\Components\\SideDrawer.js",[],"D:\\sunil\\FullStackProject\\Jigsaw FSIT MERN\\mineit\\frontend\\src\\Components\\Product.js",[],"D:\\sunil\\FullStackProject\\Jigsaw FSIT MERN\\mineit\\frontend\\src\\Components\\CartItem.js",[],"D:\\sunil\\FullStackProject\\Jigsaw FSIT MERN\\mineit\\frontend\\src\\redux\\store.js",[],"D:\\sunil\\FullStackProject\\Jigsaw FSIT MERN\\mineit\\frontend\\src\\reducers\\cartReducers.js",[],"D:\\sunil\\FullStackProject\\Jigsaw FSIT MERN\\mineit\\frontend\\src\\constants\\cartConstants.js",[],"D:\\sunil\\FullStackProject\\Jigsaw FSIT MERN\\mineit\\frontend\\src\\reducers\\productReducers.js",[],"D:\\sunil\\FullStackProject\\Jigsaw FSIT MERN\\mineit\\frontend\\src\\constants\\productConstan